<%- if controller_name != 'sessions' %>
  <%= link_to "Ir para início", new_session_path(resource_name), class: 'btn btn-secondary btn-sm' %>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Faça seu registro", new_registration_path(resource_name), class: 'btn btn-secondary btn-sm' %>
<% end -%>